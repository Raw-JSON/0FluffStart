{
  "manifest_version": 3,
  "name": "0flufStart",
  "version": "3.0.0",
  "description": "Zero-Fluff. All Speed. A privacy-first, minimalist start page.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "128": "icon.png"
  }

}
